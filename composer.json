{
    "name": "soulroll/silverstripe-eklektos",
    "type": "components-library",
    "description": "A set of commonly used components for SilverStripe to start projects faster",
    "require": {
        "php": ">=5.6.0",
        "silverstripe/recipe-plugin": "^1.2",
        "silverstripe/recipe-cms": "4.3.0@stable",
        "dnadesign/silverstripe-elemental": "^4",
        "dnadesign/silverstripe-elemental-userforms": "3.0.0-beta1",
        "silverstripe/elemental-fileblock": "^2.0",
        "silverstripe/elemental-bannerblock": "^2.0",
        "silverstripe/userforms": "^5.2",
        "silverstripe/spamprotection": "^3.0",
        "undefinedoffset/silverstripe-nocaptcha": "^2.0",
        "silverstripe/blog": "^3.2",
        "heyday/silverstripe-responsive-images": "^2.0.4",
        "silverstripe/display-logic": "^2.0",
        "undefinedoffset/sortablegridfield": "^2.0",
        "symbiote/silverstripe-gridfieldextensions": "^3",
        "heyday/silverstripe-colorpalette": "^2.0.3",
        "silverstripe/taxonomy": "^2.0",
        "silverstripe/versioned-admin": "^1.1",
        "sheadawson/silverstripe-linkable": "^2.0",
        "jonom/focuspoint": "^3.0"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/soulroll/silverstripe-bootstarter.git",
            "branch": "master"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "extra": {
        "expose": [
            "app/icons",
            "app/images"
        ],
        "project-files": [
            "app/_config/*",
            ".env.example"
        ],
        "public-files": [
            "assets/*",
            "favicon.ico"
        ],
        "project-files-installed": [
            "app/.htaccess",
            "app/_config.php",
            "app/_config/mysite.yml",
            "app/src/Page.php",
            "app/src/PageController.php"
        ],
        "public-files-installed": [
            ".htaccess",
            "index.php",
            "install-frameworkmissing.html",
            "install.php",
            "web.config"
        ]
    },
    "config": {
        "process-timeout": 600
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
